#/bin/sh
#
# Author: Bas van der Vlies
# Date  : 28 Juni 2010
# Desc. : Copy the needed libraries in the initrd. A simple mklibs replacement
#
# SVn Info:
# 	$Id$
#       $URL$

if [ $# -lt 1 ]
then
	echo "Usage: $(basename $0) <executable> [excutables]"
	exit 0
fi

FILES=$@

for f in  $FILES
do
	ldd $f  | awk '{ print $3 }' | grep ^/
done | sort | uniq 
