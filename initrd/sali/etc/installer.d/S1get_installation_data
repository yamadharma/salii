
p_stage "Retrieving installation data"

p_service "Retrieving scripts directory"
rsync -a ${IMAGESERVER}::scripts/ /scripts/

if [ "x${PROTOCOL}" == "xbittorrent" -o "x${PROTOCOL}" == "xmetafile" ]
then
    p_service "Retrieving torrents directory"
	rsync -a ${IMAGESERVER}::torrents/ /torrents/
fi
