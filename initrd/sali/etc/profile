export PATH="/bin:/sbin:/usr/bin:/usr/sbin"
export MANPATH="$MANPATH:/usr/local/share/man:/usr/man:/usr/share/man"
export SHELL="/bin/ash"
export LD_LIBRARY_PATH=/lib:/usr/lib

ulimit -c 0
umask 022

if [ $(id -u) -eq 0 ]
then                 
   export PS1="\h:\w \u\# "
else
   export PS1="\h:\w \u\$ " 
fi
