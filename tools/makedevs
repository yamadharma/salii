if [ "$RPM_ARCH" != s390 ]; then

# ----------------
# Major number 202
# ----------------

for i in 0 1 2 3 4 5 6 7
do
	mknod -m 600 $DEV/cpu/$i/msr c 202 $i
done

# ----------------
# Major number 203
# ----------------

for i in 0 1 2 3 4 5 6 7
do
	mknod -m 444 $DEV/cpu/$i/cpuid c 203 $i
done

# ----------------
# Major number 204
# ----------------

create_dev         ttySG0     204     40    c     1   660     root:root

fi # not s390

