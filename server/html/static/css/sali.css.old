/* SALI CSS, based on the theme SB Admin from http://startbootstrap.com */


body {
  margin-top: 50px;
}

#wrapper {
  padding-left: 0;
}

#page-wrapper {
  width: 100%;
  padding: 5px 15px;
}


/* Nav Announcements */

.announcement-heading {
  font-size: 50px;
  margin: 0;
}

.announcement-text {
  margin: 0;
}

/* Flot Chart Containers */

.flot-chart {
  display: block;
  height: 400px;
}

.flot-chart-content {
  width: 100%;
  height: 100%;
}

/* Edit Below to Customize Widths > 768px */
@media (min-width:768px) {

  /* Wrappers */

  #wrapper {
	padding-left: 225px;
  }

  #page-wrapper {
	padding: 15px 25px;
  }

  /* Side Nav */

  .side-nav {
	margin-left: -225px;
	left: 225px;
	width: 225px;
	position: fixed;
	top: 50px;
	height: 100%;
	border-radius: 0;
	border: none;
	background-color: #222222;
	overflow-y: auto;
  }

  .side-nav>li>a {
	width: 225px;
  }

  .navbar-inverse .navbar-nav>li>a:hover,
  .navbar-inverse .navbar-nav>li>a:focus {
	background-color: #080808;
  }

}

.panel-heading {
    height: 150px;
}
.panel-footer {
    height: 60px;
}

.table > thead > tr > .rsync_progress,
.table > tbody > tr > .rsync_progress,
.table > tfoot > tr > .rsync_progress,
.table > thead > .rsync_progress > td,
.table > tbody > .rsync_progress > td,
.table > tfoot > .rsync_progress > td,
.table > thead > .rsync_progress > th,
.table > tbody > .rsync_progress > th,
.table > tfoot > .rsync_progress > th {
  background-color: #ccffff;
}

.table > thead > tr > .tarball_progress,
.table > tbody > tr > .tarball_progress,
.table > tfoot > tr > .tarball_progress,
.table > thead > .tarball_progress > td,
.table > tbody > .tarball_progress > td,
.table > tfoot > .tarball_progress > td,
.table > thead > .tarball_progress > th,
.table > tbody > .tarball_progress > th,
.table > tfoot > .tarball_progress > th {
  background-color: #ffff99;
}
